{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "no-use-before-define": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off" // TEMP remove after form refactor
  }
}
